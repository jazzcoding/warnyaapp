{
  "expo": {
    "name": "Warnya",
    "slug": "warnya",
    "version": "3.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/warnya_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
 
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.warnya.warnya",
      "config":{
        "googleMobileAdsAppId":"ca-app-pub-5356140400244515~2938550185"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app use camera to capture picture for your profile picture and  to upload photos from your camera app and/or scan QR codes used for group invites",
        "UIInterfaceOrientationPortrait": true,
        "UIRequiresFullScreen": true,
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library to upload your profile photo"

      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"

      },
      "package": "com.warnya.warnya",
      "permissions":["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "NOTIFICATIONS","CAMERA", "CAMERA_ROLL"],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5356140400244515~5982263882"
      },
      "versionCode": 11
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
